
{
  "name": "Moodle Login",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action" : {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js", "settings.js"],
    "persistent": false
  },
  "permissions": [
      "activeTab",
      "https://moodle.tau.ac.il/*",
      "https://nidp.tau.ac.il/nidp/*"
  ]
}